{
  "name": "true-modules",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "Ports, specs, composer and scaffolds for True Modules.",
  "bin": {
    "tm": "./tm.mjs"
  },
  "scripts": {
    "schema:compile": "node tm.mjs schema-compile",
    "compose": "node tm.mjs compose --compose ./examples/compose.json --modules-root ./examples/modules --out ./examples/winner",
    "meta": "node tm.mjs meta --coverage ./examples/coverage.json --out ./examples/compose.greedy.json",
    "module": "node tm.mjs module --new"
  },
  "devDependencies": {
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "commander": "^11.1.0"
  }
}